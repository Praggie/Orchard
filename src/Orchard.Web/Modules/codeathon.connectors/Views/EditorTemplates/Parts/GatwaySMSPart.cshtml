            @Html.ValidationMessageFor(m => m.SMSError, "*")
        </div>
        <div>
            @Html.LabelFor(m => m.SMSDirection, @T("SMSDirection"))
            @Html.EditorFor(m => m.SMSDirection)
            @Html.ValidationMessageFor(m => m.SMSDirection, "*")
        </div>

</fieldset>

