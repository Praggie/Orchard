            @Html.ValidationMessageFor(m => m.SMSBody, "*")
        </div>
        <div>
            @Html.LabelFor(m => m.SMSStatus, @T("SMSStatus"))
            @Html.EditorFor(m => m.SMSStatus)
            @Html.ValidationMessageFor(m => m.SMSStatus, "*")
        </div>
        <div>
            @Html.LabelFor(m => m.SMSError, @T("SMSError"))
